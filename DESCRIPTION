Package: SeuratDisk
Type: Package
Title: Interfaces for HDF5-Based Single Cell File Formats
Version: 0.0.0.9013
Date: 2020-08-30
Authors@R: c(
  person(given = 'Paul', family = 'Hoffman', email = 'phoffman@nygenome.org', role = c('aut', 'cre'), comment = c(ORCID = '0000-0002-7693-8957')),
